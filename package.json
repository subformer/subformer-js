{
  "name": "subformer",
  "version": "0.1.0",
  "description": "Official TypeScript/JavaScript SDK for Subformer - AI-powered video dubbing and voice cloning",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "subformer",
    "video-dubbing",
    "voice-cloning",
    "text-to-speech",
    "tts",
    "ai",
    "translation",
    "audio",
    "video",
    "youtube",
    "tiktok",
    "instagram"
  ],
  "author": "Subformer <support@subformer.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/subformer/subformer-js.git"
  },
  "homepage": "https://subformer.com",
  "bugs": {
    "url": "https://github.com/subformer/subformer-js/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "eslint": "^8.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
